<?xml version="1.0" encoding="utf-8"?>
<!--suppress ALL -->
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:map="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@id/constraint"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Create_An_Account_And_Sign_In.Info.Map_Activity">

    <SearchView
        android:id="@+id/Search_Edit_Text"

        android:theme="@style/Map_Search_Edit_Text_Style"
        style="@style/Map_Search_Edit_Text_Style"

        android:layout_width="match_parent"
        android:layout_height="@dimen/_31sdp"

        android:layout_marginTop="@dimen/_8sdp"
        android:layout_marginStart="@dimen/_4sdp"
        android:layout_marginEnd="@dimen/_46sdp"

        android:background="@drawable/shapes_map_search_edit_text_shape"

        android:iconifiedByDefault="false"

        android:queryHint="بحث"

        map:layout_constraintBottom_toBottomOf="parent"
        map:layout_constraintEnd_toEndOf="parent"
        map:layout_constraintHorizontal_bias="0.5"
        map:layout_constraintStart_toStartOf="parent"
        map:layout_constraintTop_toTopOf="parent"
        map:layout_constraintVertical_bias="0.0" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        map:layout_constraintBottom_toBottomOf="parent"
        map:layout_constraintEnd_toEndOf="parent"
        map:layout_constraintHorizontal_bias="0.5"
        map:layout_constraintStart_toStartOf="parent"
        map:layout_constraintTop_toTopOf="parent"
        map:layout_constraintVertical_bias="0.5" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/Save_Workplace_On_Map_BTN"

        android:layout_width="match_parent"
        android:layout_height="@dimen/_31sdp"

        android:layout_marginTop="@dimen/_543sdp"
        android:layout_marginStart="@dimen/_4sdp"
        android:layout_marginEnd="@dimen/_46sdp"

        android:background="@drawable/shapes_map_search_edit_text_shape"

        android:textColor="@color/white"
        android:textSize="@dimen/_15sdp"
        android:text="حفظ المكان"

        android:onClick="Save_Workplace_On_Map_BTN"

        map:layout_constraintBottom_toBottomOf="parent"
        map:layout_constraintEnd_toEndOf="parent"
        map:layout_constraintHorizontal_bias="0.498"
        map:layout_constraintStart_toStartOf="parent"
        map:layout_constraintTop_toTopOf="parent"
        map:layout_constraintVertical_bias="0.0"
        tools:ignore="HardcodedText,TouchTargetSizeCheck,TextContrastCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>